<?xml version="1.0" encoding="UTF-8"?>
<generalAdapterRun xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/generalAdapterRun.xsd">
	<general>
		<rootDir>%REGION_HOME%/Modules/wflow</rootDir>
		<workDir>%ROOT_DIR%/$_WFLOW_MODEL$/</workDir>
		<exportDir>%WORK_DIR%/inmaps</exportDir>
		<exportDataSetDir>%WORK_DIR%</exportDataSetDir>
		<exportIdMap>IdExportWFLOW</exportIdMap>
		<importDir>%WORK_DIR%/run_default/outmaps</importDir>
		<importIdMap>IdImportWFLOW</importIdMap>
		<dumpFileDir>%REGION_HOME%/Modules/dump_files</dumpFileDir>
		<dumpDir>%WORK_DIR%</dumpDir>
		<diagnosticFile>%WORK_DIR%/wflow_diag.xml</diagnosticFile>
		<missVal>-999</missVal>
		<convertDatum>true</convertDatum>
	</general>
	<activities>
		<startUpActivities>
			<purgeActivity>
				<filter>%WORK_DIR%/inmaps/*.nc</filter>
			</purgeActivity>
			<purgeActivity>
				<filter>%WORK_DIR%\run_default\outmaps\*.*</filter>
			</purgeActivity>
			<purgeActivity>
				<filter>%WORK_DIR%\run_default\outstate\*.*</filter>
			</purgeActivity>
			<purgeActivity>
				<filter>%WORK_DIR%\run_default\outsum\*.*</filter>
			</purgeActivity>
		</startUpActivities>
		<exportActivities>
			<exportStateActivity>
				<moduleInstanceId>$_WARM_STATE_MODULE_INSTANCE$</moduleInstanceId>
				<stateExportDir>%WORK_DIR%/instate</stateExportDir>
				<stateConfigFile>%WORK_DIR%/instate/state.xml</stateConfigFile>
				<stateLocations type="file">
					<stateLocation>
						<readLocation>DrySnow1.map</readLocation>
						<writeLocation>../run_default/outstate/DrySnow1.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>DrySnow2.map</readLocation>
						<writeLocation>../run_default/outstate/DrySnow2.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>EVI1.map</readLocation>
						<writeLocation>../run_default/outstate/EVI1.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>EVI2.map</readLocation>
						<writeLocation>../run_default/outstate/EVI2.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>FreeWater1.map</readLocation>
						<writeLocation>../run_default/outstate/FreeWater1.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>FreeWater2.map</readLocation>
						<writeLocation>../run_default/outstate/FreeWater2.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>LAI1.map</readLocation>
						<writeLocation>../run_default/outstate/LAI1.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>LAI2.map</readLocation>
						<writeLocation>../run_default/outstate/LAI2.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>Mleaf1.map</readLocation>
						<writeLocation>../run_default/outstate/Mleaf1.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>Mleaf2.map</readLocation>
						<writeLocation>../run_default/outstate/Mleaf2.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>S01.map</readLocation>
						<writeLocation>../run_default/outstate/S01.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>S02.map</readLocation>
						<writeLocation>../run_default/outstate/S02.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>Sd1.map</readLocation>
						<writeLocation>../run_default/outstate/Sd1.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>Sd2.map</readLocation>
						<writeLocation>../run_default/outstate/Sd2.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>Sg.map</readLocation>
						<writeLocation>../run_default/outstate/Sg.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>Sr.map</readLocation>
						<writeLocation>../run_default/outstate/Sr.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>Ss1.map</readLocation>
						<writeLocation>../run_default/outstate/Ss1.map</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>Ss2.map</readLocation>
						<writeLocation>../run_default/outstate/Ss2.map</writeLocation>
					</stateLocation>
				</stateLocations>
				<stateSelection>
					<warmState>
						<stateSearchPeriod unit="day" start="$_WARM_STATE_START$" end="$_REL_VIEW_PERIOD_START$"/>
					</warmState>
				</stateSelection>
			</exportStateActivity>
			<exportDataSetActivity>
				<moduleInstanceId>$_WFLOW_MODEL$</moduleInstanceId>
			</exportDataSetActivity>
			<exportNetcdfActivity>
				<exportFile>inmaps.nc</exportFile>
				<timeSeriesSets>
					<timeSeriesSet>
						<moduleInstanceId>preprocess_$_WFLOW_MODEL$_update_$_DATA_PROD$</moduleInstanceId>
						<valueType>grid</valueType>
						<parameterId>tp</parameterId>
						<locationId>$_WFLOW_MODEL$</locationId>
						<timeSeriesType>external historical</timeSeriesType>
						<timeStep unit="hour" multiplier="$_WFLOW_MODEL_TEMPORAL_RES$"/>
						<relativeViewPeriod unit="day" start="$_REL_VIEW_PERIOD_START$" end="$_REL_VIEW_PERIOD_END$" />
						<readWriteMode>read only</readWriteMode>
					</timeSeriesSet>
					<timeSeriesSet>
						<moduleInstanceId>preprocess_$_WFLOW_MODEL$_update_$_DATA_PROD$</moduleInstanceId>
						<valueType>grid</valueType>
						<parameterId>2t</parameterId>
						<locationId>$_WFLOW_MODEL$</locationId>
						<timeSeriesType>external historical</timeSeriesType>
						<timeStep unit="hour" multiplier="$_WFLOW_MODEL_TEMPORAL_RES$"/>
						<relativeViewPeriod unit="day" start="$_REL_VIEW_PERIOD_START$" end="$_REL_VIEW_PERIOD_END$" />
						<readWriteMode>read only</readWriteMode>
					</timeSeriesSet>
					<timeSeriesSet>
						<moduleInstanceId>preprocess_$_WFLOW_MODEL$_update_$_DATA_PROD$</moduleInstanceId>
						<valueType>grid</valueType>
						<parameterId>E.ref.mak</parameterId>
						<locationId>$_WFLOW_MODEL$</locationId>
						<timeSeriesType>external historical</timeSeriesType>
						<timeStep unit="hour" multiplier="$_WFLOW_MODEL_TEMPORAL_RES$"/>
						<relativeViewPeriod unit="day" start="$_REL_VIEW_PERIOD_START$" end="$_REL_VIEW_PERIOD_END$" />
						<readWriteMode>read only</readWriteMode>
					</timeSeriesSet>
				</timeSeriesSets>
			</exportNetcdfActivity>
			<exportRunFileActivity>
				<exportFile>%WORK_DIR%/runinfo.xml</exportFile>
			</exportRunFileActivity>
		</exportActivities>
		<executeActivities>
			<executeActivity>
				<description>Run wflow</description>
				<command>
					<executable>wflow-$_WFLOW_BUNDLE$-bundle\wflow_w3ra.exe</executable>
				</command>
				<arguments>
					<argument>-C</argument>
					<argument>.</argument>
				</arguments>
				<timeOut>44200000</timeOut>
				<ignoreDiagnostics>true</ignoreDiagnostics>
			</executeActivity>
			<executeActivity>
				<description>Run wflow post</description>
				<command>
					<executable>wflow-$_WFLOW_BUNDLE$-bundle\wflow_adapt.exe</executable>
				</command>
				<arguments>
					<argument>-M</argument>
					<argument>Post</argument>
					<argument>-s</argument>
					<argument>instate/state.xml</argument>
					<argument>-o</argument>
					<argument>outstate/outstate.xml</argument>
					<argument>-r</argument>
					<argument>./runinfo.xml</argument>
					<argument>-w</argument>
					<argument>./</argument>
					<argument>-C</argument>
					<argument>.</argument>
					<argument>-I</argument>
					<argument>wflow_W3RA.ini</argument>
				</arguments>
				<timeOut>1200000</timeOut>
				<ignoreDiagnostics>true</ignoreDiagnostics>
			</executeActivity>
		</executeActivities>
		<importActivities>
			<importNetcdfActivity>
				<importFile>%WORK_DIR%/run_default/outmaps.nc</importFile>
				<timeSeriesSets>
					<timeSeriesSet>
						<moduleInstanceId>$_WFLOW_MODEL$_update_$_DATA_PROD$</moduleInstanceId>
						<valueType>grid</valueType>
						<parameterId>Q.noRouted</parameterId>
						<locationId>$_WFLOW_MODEL$</locationId>
						<timeSeriesType>simulated historical</timeSeriesType>
						<timeStep unit="hour" multiplier="$_WFLOW_MODEL_TEMPORAL_RES$"/>
						<readWriteMode>add originals</readWriteMode>
						<expiryTime unit="hour" multiplier="$_EXPIRY_TIME$"/>
					</timeSeriesSet>
				</timeSeriesSets>
			</importNetcdfActivity>
		</importActivities>
	</activities>
</generalAdapterRun>
